{
  "name": "groundschool-ai",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "prestart": "node scripts/ensure-node-version.js",
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:sentry": "node scripts/test-sentry-cli.js",
    "verify:sentry": "node scripts/verify-sentry-node.js",
    "clean": "./scripts/clean-project.sh",
    "update-deps": "./scripts/update-dependencies.sh",
    "check-deps": "node scripts/check-dependency-conflicts.js",
    "lint": "expo lint",
    "prebuild": "node scripts/pre-build-checks.js",
    "build:dev": "npm run prebuild && eas build --profile development --platform all",
    "build:preview": "npm run prebuild && eas build --profile preview --platform all",
    "build:production": "npm run prebuild && eas build --profile production --platform all",
    "build:android": "npm run prebuild && eas build --profile production --platform android",
    "build:ios": "npm run prebuild && eas build --profile production --platform ios",
    "build:sentry": "./scripts/build-with-sentry.sh",
    "build:sentry:dev": "./scripts/build-with-sentry.sh --profile development",
    "build:sentry:preview": "./scripts/build-with-sentry.sh --profile preview",
    "build:sentry:prod": "./scripts/build-with-sentry.sh --profile production",
    "update:production": "eas update --channel production",
    "update:preview": "eas update --channel preview"
  },
  "dependencies": {
    "@expo/config": "^10.0.10",
    "@expo/vector-icons": "^14.0.0",
    "@invertase/react-native-apple-authentication": "^2.4.0",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-google-signin/google-signin": "^13.2.0",
    "@react-navigation/drawer": "^7.1.1",
    "@react-navigation/native": "^7.0.14",
    "@sentry/react-native": "~6.3.0",
    "@supabase/supabase-js": "^2.49.1",
    "ajv": "^8.17.1",
    "axios": "^1.6.7",
    "bcrypt": "^5.1.1",
    "expo": "~52.0.38",
    "expo-apple-authentication": "^7.1.3",
    "expo-auth-session": "^6.0.3",
    "expo-constants": "~17.0.7",
    "expo-dev-client": "~5.0.14",
    "expo-dev-launcher": "^5.0.31",
    "expo-document-picker": "~13.0.3",
    "expo-file-system": "~18.0.11",
    "expo-font": "~13.0.4",
    "expo-image-manipulator": "^13.0.6",
    "expo-image-picker": "~16.0.6",
    "expo-linking": "~7.0.5",
    "expo-router": "~4.0.19",
    "expo-splash-screen": "~0.29.22",
    "expo-status-bar": "~2.0.1",
    "expo-system-ui": "^4.0.8",
    "expo-updates": "~0.27.4",
    "expo-web-browser": "^14.0.2",
    "nanoid": "^3.3.4",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.7",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-keychain": "^9.2.3",
    "react-native-paper": "^5.12.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-vector-icons": "^10.0.3",
    "react-native-web": "~0.19.10",
    "styled-components": "^6.1.16",
    "zustand": "^4.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/runtime": "^7.24.0",
    "@jest/globals": "^29.7.0",
    "@sentry/node": "^9.6.1",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react": "^14.2.1",
    "@testing-library/react-native": "^12.4.3",
    "@types/react": "~18.3.12",
    "axios-mock-adapter": "^2.1.0",
    "babel-jest": "^29.7.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "dotenv": "^16.4.7",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~8.0.1",
    "expect": "^29.7.0",
    "jest": "^29.7.0",
    "jest-expo": "~52.0.6",
    "metro-react-native-babel-preset": "0.77.0",
    "react-native-dotenv": "^3.4.11",
    "react-test-renderer": "18.3.1",
    "typescript": "^5.8.2"
  },
  "jest": {
    "preset": "jest-expo",
    "transformIgnorePatterns": [
      "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|react-native-svg|expo-router)"
    ],
    "setupFilesAfterEnv": [
      "./jest.setup.js"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx,ts,tsx}",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/jest.setup.js"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "listUnknownPackages": false
      }
    }
  },
  "resolutions": {
    "@types/react": "~18.3.12",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-native": "0.76.7",
    "react-test-renderer": "18.3.1"
  }
}
